{
  "author": "旧日丨四季列车",
  "version": "v0.0.1",
  "descript": "通过表格统计你的笔记，双击在 Obsidian 中打开",
  "model": {
    "width": {
      "type": "int",
      "name": "数据库宽度",
      "default": 540
    },
    "height": {
      "type": "int",
      "name": "数据库高度",
      "default": 250
    },
    "noteFilter": {
      "type": "noteFilter",
      "name": "笔记过滤",
      "default": {
        "match": "all",
        "filters": []
      }
    },
    "noteProp": {
      "type": "noteProp",
      "name": "笔记属性",
      "default": {
        "props": [{
          "dataKey": "file.name",
          "title": "名称",
          "width": 300
        }, {
          "dataKey": "file.mtime",
          "title": "修改日期",
          "width": 200
        }]
      }
    }
  },
  "option": {
    "items": [],
    "tabs": [{
      "id": "filter",
      "text": "筛选",
      "items": ["noteFilter"]
    }, {
      "id": "prop",
      "text": "属性",
      "items": ["noteProp"]
    }]
  }
}
